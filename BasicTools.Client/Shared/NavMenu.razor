@inject RouteSourceAssemblyProvider RouteSources

<MudNavMenu>
    <MudNavLink Href="" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.Home">Home</MudNavLink>

    @foreach (var tool in RouteSources.ToolPages)
    {
        <MudNavLink Href="@tool.Key" Icon="@tool.Value.GetMudIcon()">@tool.Value.Name</MudNavLink>
    }

</MudNavMenu>